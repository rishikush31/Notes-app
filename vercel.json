{
    "version":2,
    "build":[
        {
            "src":"index.js",
            "use":"@vercel/node"
        },
        {
            "src":"build/",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {
        "src":"/api",
        "dest":"index.js"
        },
        {
        "src":"/(.*)",
        "dest":"build/"
        }
    ]
}